{
  "name": "bananalight",
  "type": "light",
  "tokenColors": [
    {
      "name": "Syntax, Decorators",
      "scope": [
        "storage",
        "keyword",
        "variable.other.readwrite.decorator"
      ],
      "settings": {
        "foreground": "#822ebe"
      }
    },
    {
      "name": "Variables",
      "scope": "variable",
      "settings": {
        "foreground": "#11095e"
      }
    },
    {
      "name": "Functions, Classes, Punctuation",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "support.class.builtin",
        "source punctuation",
        "source meta.function meta.brace",
      ],
      "settings": {
        "foreground": "#1650cc"
      }
    },
    {
      "name": "Object keys, Object properties, JSON properties",
      "scope": [
        "constant.other.object.key.js",
        "variable.other.property",
        "source.json.comments meta.structure punctuation",
        "meta.brace.curly.litobj.js"
      ],
      "settings": {
        "foreground": "#5f1111"
      }
    },
    {
      "name": "JSX attributes",
      "scope": "entity.other.attribute-name.jsx",
      "settings": {
        "foreground": "#5f1111",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings, JSX tags",
      "scope": [
        "string.quoted",
        "meta.tag.jsx entity.name",
        "source meta.tag.jsx punctuation",
        "source.json.comments meta.structure string.quoted punctuation"
      ],
      "settings": {
        "foreground": "#0b7777"
      }
    },
    {
      "name": "JSX tags text content",
      "scope": [
        "meta.tag.jsx"
      ],
      "settings": {
        "foreground": "#4d4c4c"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment.line",
        "comment.block",
        "source punctuation.definition.comment",
      ],
      "settings": {
        "foreground": "#858585"
      }
    },
    {
      "name": "Flow declarations",
      "scope": [
        "punctuation.flowtype",
        "meta.object.flowtype variable",
        "meta.object.flowtype variable.other.property",
        "support.type.class.flowtype",
        "keyword.other.typedef.flowtype",
        "keyword.object.flowtype",
        "source meta.object.flowtype keyword",
        "source meta.object.flowtype meta.brace"
      ],
      "settings": {
        "foreground": "#7d7d92"
      }
    },
  ],
